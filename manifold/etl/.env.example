# Copy to .env and fill in credentials.
# MANIFOLD_API_KEY is optional; leave blank for public endpoints.
MANIFOLD_API_KEY=

# PostgreSQL connection string:
# Format: postgresql://user:password@host:5432/database
POSTGRES_URI=postgresql://user:password@host:5432/manifolddb

# Directory for pipeline logs (created automatically if missing)
LOG_DIR=logs
